<div class="header container-fluid       ">
  <div class="row d-flex p-xl-4 justify-content-between  align-items-center">

  <div class="col-md-4 d-flex   ">
    <h2>Real Time</h2>
  </div>

  <div class="col-md-3 d-flex gap-4 justify-content-end   ">
    <a href="/" class="btn btn-primary">Home</a>
    <a href="/chat" class="btn btn-primary">chat</a>

  </div>
</div>
  </div>

<hr>
<h3 class="text-center">Productos</h3>
<div class="d-flex  justify-content-center  align-content-center gap-3 p-5">


  {{#if hasPrevPage}}

  <a href="/realtimeproducts/?pagina={{prevPage}}" class="btn btn-primary ">Pag.Ant.</a>

  {{/if}}
  <a href="/realtimeproducts/?pagina=1" class="btn btn-primary ">Pag.1</a>

  {{#if hasNextPage}}

  <a href="/realtimeproducts/?pagina={{nextPage}}" class="btn btn-primary ">Pag. Sig.</a>

  {{/if}}
  <a href="/realtimeproducts/?pagina={{totalPages}}" class="btn btn-primary ">Ult. Pag.</a>

</div>


  <div   class="d-grid">
  <div id="list-group"  class="row gap-2 justify-content-center pt-5">

    {{#each productos}}
      <div class="col-sm-3">

        <div class="card">
          <div class="card-body">
            <h5 class="card-title"> {{title}}</h5>
            <p class="card-text">Descripcion: {{description}}</p>
            <p class="card-text">Precio: {{price}}</p>
          </div>
        </div>
      </div>

    {{/each}}

  </div>

</div>
<div class="d-flex  justify-content-center  align-content-center gap-3 p-5">


  {{#if hasPrevPage}}

  <a href="/realtimeproducts/?pagina={{prevPage}}" class="btn btn-primary ">Pag.Ant.</a>

  {{/if}}
  <a href="/realtimeproducts/?pagina=1" class="btn btn-primary ">Pag.1</a>

  {{#if hasNextPage}}

  <a href="/realtimeproducts/?pagina={{nextPage}}" class="btn btn-primary ">Pag. Sig.</a>

  {{/if}}
  <a href="/realtimeproducts/?pagina={{totalPages}}" class="btn btn-primary ">Ult. Pag.</a>

</div>



<script type="text/javascript" src="https://cdn.jsdelivr.net/npm/toastify-js"></script>

<script src="/socket.io/socket.io.js"></script>
<script src="/js/realTime.js"></script>